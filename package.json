{
  "name": "sanity-plugin-dashboard-widget-netlify",
  "version": "1.3.0",
  "description": "Sanity Studio Dashboard Widget for triggering Netlify builds",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "lib/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "files": [
    "lib/",
    "sanity.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "prepublishOnly": "npm run compile && npm run build",
    "prepare": "husky install",
    "compile": "tsc --noEmit",
    "clean": "rimraf lib",
    "link-watch": "plugin-kit link-watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-dashboard-widget-netlify.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "sanity",
    "plugin",
    "dashboard",
    "widget",
    "netlify"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify/issues"
  },
  "homepage": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.0",
    "abort-controller": "^2.0.3",
    "react-props-stream": "^1.0.0",
    "rxjs": "^6.0.0"
  },
  "peerDependencies": {
    "@sanity/dashboard": "^3.0.0-v3-studio.1",
    "@sanity/ui": "^0.36.12",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@sanity/dashboard": "^3.0.0-v3-studio.1",
    "@sanity/plugin-kit": "^1.0.1",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@sanity/ui": "^0.38.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^10.11.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^26.0.0",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^13.0.3",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^2.6.2",
    "sanipack": "^1.0.8",
    "sanity": "3.0.0-dev-preview.15",
    "styled-components": "^5.2.0",
    "ts-jest": "^26.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "4.7.4"
  }
}
